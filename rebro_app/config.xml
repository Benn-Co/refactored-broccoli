<?xml version='1.0' encoding='utf-8'?>
<widget id="com.benco.org" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Rebro</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    <splash src="/app/src/main/assets/www/image/android-chrome-512x512.png" density="land-hdpi" />
    <splash src="/app/src/main/assets/www/image/android-chrome-192x192.png" density="land-ldpi" />
    <splash src="/app/src/main/assets/www/image/apple-touch-icon.png" density="land-mdpi" />
    <splash src="/app/src/main/assets/www/image/favicon-32x32.png" density="land-xhdpi" />
    <splash src="/app/src/main/assets/www/image/favicon-32x32.png" density="land-xxhdpi" />
    <splash src="/app/src/main/assets/www/image/favicon-16x16.png" density="land-xxxhdpi" />

    <splash src="/app/src/main/assets/www/image/android-chrome-512x512.png" density="port-hdpi" />
    <splash src="/app/src/main/assets/www/image/android-chrome-192x192.png" density="port-ldpi" />
    <splash src="/app/src/main/assets/www/image/apple-touch-icon.png" density="port-mdpi" />
    <splash src="/app/src/main/assets/www/image/favicon-32x32.png" density="port-xhdpi" />
    <splash src="/app/src/main/assets/www/image/favicon-32x32.png" density="port-xxhdpi" />
    <splash src="/app/src/main/assets/www/image/favicon-16x16.png" density="port-xxxhdpi" /> 
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
